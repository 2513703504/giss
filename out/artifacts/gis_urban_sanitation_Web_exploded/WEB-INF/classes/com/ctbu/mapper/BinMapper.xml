<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--mapper命名空间，必须配置属性，已存在Mapper接口的全类名-->
<mapper namespace="com.ctbu.mapper.BinMapper">
    <!--显示所有垃圾箱-->
    <select id="getAllBins" resultType="Bin">
        select id,bin_number binNumber,bin_address binAddress from bin
    </select>
</mapper>